        public void addtable(int id, String Product, int Qty, double Price){
            DefaultTableModel dt = (DefaultTableModel) jTable1.getModel();
            
            for (int row = 0; row < jTable1.getRowCount();row++){
                if (Product == jTable1.getValueAt(row, 1)){
                    dt.removeRow(jTable1.convertRowIndexToModel(row));
                }
            }
            Vector v = new Vector() ;
            v.add(id);
            v.add(Product);
            v.add(Qty);
            v.add(Price);
            
            dt.addRow(v);
        }
        public void cal(){
            int tot = Integer.valueOf(total.getText());
            tot += 1;
        }
